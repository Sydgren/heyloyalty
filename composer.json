{
  "name" : "hughwilly/heyloyalty",
  "description" : "Helper for Laravel 5 that allows easy management of a HeyLoyalty member list.",
  "require" : {
    "php" : "^5.6|^7.0",
    "heyloyalty/hl-phpclient" : "^1.5.0",
    "illuminate/support" : "^5.0",
    "nesbot/carbon" : "~1.21"
  },
  "require-dev" : {
    "mockery/mockery" : "^0.9.5",
    "phpunit/phpunit" : "^5.6",
    "laravel/laravel" : "dev-develop"
  },
  "minimum-stability" : "dev",
  "autoload" : {
    "psr-4" : {
      "Hughwilly\\HeyLoyalty\\": "src/"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "Hughwilly\\HeyLoyalty\\Tests\\" : "tests/"
    },
    "files" : [
      "vendor/laravel/framework/src/Illuminate/Foundation/helpers.php"
    ]
  },
  "authors" : [ {
    "name" : "Henrik Nordquist",
    "email" : "henrik@henriknordquist.dk"
  } ]
}